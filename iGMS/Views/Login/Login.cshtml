    </div>
    <div class="form-container" style="min-height: 500px">
        <!--begin:Aside-->
        <div id="aside">
            <div class="image-layer">
                <img src="~/assets/media/login/white-outline.png" class="formm-image-main" />
                <img src="~/assets/media/login/dots.png" class="formm-image dots" />
                <img src="~/assets/media/login/coin.png" class="formm-image coin" />
                <img src="~/assets/media/login/spring.png" class="formm-image spring" />
                <img src="~/assets/media/login/warehouse.png" class="formm-image warehouse" />
                <img src="~/assets/media/login/cloud.png" class="formm-image cloud" />
                <img src="~/assets/media/login/stars.png" class="formm-image stars" />
            </div>
            <p class="featured-words">@Resources.Resource.login_welcome</p>
            <p class="bold-words">RWMS</p>
        </div>
        <!--end:Aside-->
        <!--begin:Content-->
        <div id="col2">
            <div class="login-form">
                <!--begin:Sign In Form-->
                <div class="text-center mb-10">
                    <h2 class="form-title">@Resources.Resource.login</h2>
                </div>
                <form class="form-inputs form" id="kt_login_signin_form">
                    <div class="input-box form-group">
                        <input class="input-field" type="text" placeholder="@Resources.Resource.username" id="username" name="user" autocomplete="off" required />
                        <i class="flaticon2-user icon"></i>
                    </div>
                    <div class="input-box form-group">
                        <input class="input-field" type="password" id="password" placeholder="@Resources.Resource.password" name="pass" autocomplete="off" required />
                        <i class="fas fa-lock icon"></i>
                    </div>
                    <div class="text-center mt-15 form-group">
                        <button id="kt_login_signin_submit" type="submit" class="btn btn-primary btn-pill shadow-sm py-4 px-9 font-weight-bold">@Resources.Resource.login</button>
                    </div>
                </form>
                <!--end:Forgot Password Form-->
            </div>
            <div class="dropdown">
                <!--begin::Toggle-->
                <div class="topbar-item" data-toggle="dropdown" data-offset="10px,0px" aria-expanded="false">
                    <div class="btn btn-icon btn-clean btn-dropdown btn-lg mr-1">
                        <img id="languageImage" class="h-20px w-20px rounded-sm" src="assets/media/svg/flags/220-vietnam.svg" alt="">
                    </div>
                </div>
                <!--end::Toggle-->
                <!--begin::Dropdown-->
                <div class="dropdown-menu p-0 m-0 dropdown-menu-anim-up dropdown-menu-sm dropdown-menu-right" style="">
                    <!--begin::Nav-->
                    <ul class="navi navi-hover py-4">
                        <li class="navi-item active">
                            <a href="" class="navi-link change-language" data-culture="zh-CN">
                                <span class="symbol symbol-20 mr-3">
                                    <img src="/assets/media/svg/flags/015-china.svg" alt="">
                                </span>
                                <span class="navi-text">@Resources.Resource.Chinese</span>
                            </a>
                        </li>
                        <!--begin::Item-->
                        <li class="navi-item">
                            <a href="" class="navi-link change-language" data-culture="vi">
                                <span class="symbol symbol-20 mr-3">
                                    <img src="/assets/media/svg/flags/220-vietnam.svg" alt="">
                                </span>
                                <span class="navi-text">@Resources.Resource.Vietnamese</span>
                            </a>
                        </li>
                        <!--end::Item-->
                        <!--begin::Item-->
                        <li class="navi-item active">
                            <a href="" class="navi-link change-language" data-culture="en">
                                <span class="symbol symbol-20 mr-3">
                                    <img src="/assets/media/svg/flags/226-united-states.svg" alt="">
                                </span>
                                <span class="navi-text">@Resources.Resource.English</span>
                            </a>
                        </li>
                        <li class="navi-item">
                            <a href="" class="navi-link change-language" data-culture="th">
                                <span class="symbol symbol-20 mr-3">
                                    <img src="/assets/media/svg/flags/238-thailand.svg" alt="">
                                </span>
                                <span class="navi-text">@Resources.Resource.Thailand</span>
                            </a>
                        </li>
                        <!--end::Item-->
                    </ul>
                    <!--end::Nav-->
                </div>
                <!--end::Dropdown-->
            </div>
        </div>
        <!--end:Content-->
    </div>
    <!--end::Main-->
    <!--begin::Global Config(global config for global JS scripts)-->
    <!--end::Global Config-->
    <!--begin::Global Theme Bundle(used by all pages)-->
    <script src="~/assets/plugins/global/plugins.bundle.js"></script>
    <script src="~/assets/plugins/custom/prismjs/prismjs.bundle.js"></script>
    <script src="~/assets/js/scripts.bundle.js"></script>
    @Scripts.Render("~/bundles/bootstrap")

    <!--end::Global Theme Bundle-->
    <!--begin::Page Scripts(used by this page)-->
    <!--end::Page Scripts-->
<script>
        var resources = {
            pass_not_empty: "@Html.Raw(Resources.Resource.account_error_enter_pass)",
            username_not_empty: "@Html.Raw(Resources.Resource.account_error_account)",
            require_all: "@Html.Raw(Resources.Resource.account_require)",
    }

    var currentCulture = '@(Session[Common.Currentculture] ?? "zh-CN")';
    if (localStorage.getItem("language") === null) {
        localStorage.setItem("language", currentCulture)
    }

    var languageImage = document.getElementById('languageImage');
    if (currentCulture === 'vi') {
        languageImage.src = '/assets/media/svg/flags/220-vietnam.svg';
    } else if (currentCulture === 'en') {
        languageImage.src = '/assets/media/svg/flags/226-united-states.svg';
    } else if (currentCulture === 'th') {
        languageImage.src = '/assets/media/svg/flags/238-thailand.svg';
    } else {
        languageImage.src = '/assets/media/svg/flags/015-china.svg';
    }
</script>
    <script src="~/assets/js/Login/login.js"></script>
</body>
<!--end::Body-->
</html>