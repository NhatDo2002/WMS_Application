            background-position: center;
            background-size: cover;
            background-attachment: fixed;
            background-repeat: no-repeat;
            overflow: hidden;
        }

            body.modal-open {
                overflow-x: hidden !important;
            }

        .notification.status-false {
            opacity: 0.5; /* Áp dụng hiệu ứng mờ cho các thông báo có status là false */
        }

        .nav-tabs {
            display: flex;
            flex-wrap: nowrap; /* Không cho phép tab bị gãy dòng */
            white-space: nowrap; /* Đảm bảo tất cả các tab nằm trên một dòng */
            -webkit-overflow-scrolling: touch; /* Cho phép cuộn mượt mà trên các thiết bị di động */
            scrollbar-color: inherit;
            scrollbar-width: inherit; /* Đặt độ rộng thanh cuộn nhỏ hơn trên Firefox */
        }

        .offcanvas-header-navs {
            height: auto !important; /* Đặt chiều cao tự động để không gây ra cuộn dọc */
        }

        .nav-item {
            flex: 0 0 auto; /* Mỗi tab có kích thước cố định và không co giãn */
            text-align: center; /* Canh giữa văn bản của tab */
        }

        .nav-link {
            white-space: nowrap; /* Tránh việc văn bản trong tab bị gãy dòng */
        }

        .nav-tabs::-webkit-scrollbar {
            height: 8px; /* Chiều cao thanh cuộn ngang */
        }

        .nav-tabs::-webkit-scrollbar-thumb {
            background-color: #888; /* Màu của thanh cuộn */
            border-radius: 10px; /* Bo tròn góc */
        }

            .nav-tabs::-webkit-scrollbar-thumb:hover {
