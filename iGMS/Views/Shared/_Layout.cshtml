            }

        .nav-tabs::-webkit-scrollbar-track {
            background: #eee; /* Nền thanh cuộn */
        }

        .offcanvas.offcanvas-right {
            right: -521px;
        }

        .menu-text {
            color: #000 !important;
            font-weight: 500 !important;
        }

        #kt_content {
            padding-top: 10%
        }

        #kt_wrapper {
            padding-top: 0 !important
        }
